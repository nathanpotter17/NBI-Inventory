generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider  = "postgresql"
  url       = env("POSTGRES_PRISMA_URL") // uses connection pooling
  directUrl = env("POSTGRES_URL_NON_POOLING") // uses a direct connection
}

model Bridge {
  id                       Int      @id @default(autoincrement()) // Unique identifier for each row
  STATE_CODE_001           String // State code, always 42 for Pennsylvania
  STRUCTURE_NUMBER_008     String // Structure number
  RECORD_TYPE_005A         Int // Record type
  ROUTE_PREFIX_005B        Int // Route prefix
  SERVICE_LEVEL_005C       Int // Service level
  ROUTE_NUMBER_005D        String // Route number
  DIRECTION_005E           Int // Direction
  HIGHWAY_DISTRICT_002     String // Highway district
  COUNTY_CODE_003          String // County code
  PLACE_CODE_004           String // Place code
  FEATURES_DESC_006A       String? // Features description (nullable)
  CRITICAL_FACILITY_006B   String? // Critical facility (nullable)
  FACILITY_CARRIED_007     String // Facility carried
  LOCATION_009             String // Location description
  MIN_VERT_CLR_010         Float // Minimum vertical clearance
  KILOPOINT_011            Int // Kilopoint
  BASE_HWY_NETWORK_012     Int // Base highway network
  LRS_INV_ROUTE_013A       String? // LRS inventory route (nullable)
  SUBROUTE_NO_013B         String? // Subroute number (nullable)
  LAT_016                  Float // Latitude
  LONG_017                 Float // Longitude
  DETOUR_KILOS_019         Float // Detour kilometers
  TOLL_020                 Int // Toll
  MAINTENANCE_021          Int // Maintenance
  OWNER_022                Int // Owner
  FUNCTIONAL_CLASS_026     Int // Functional class
  YEAR_BUILT_027           Int // Year built
  TRAFFIC_LANES_ON_028A    Int // Traffic lanes on
  TRAFFIC_LANES_UND_028B   Int // Traffic lanes under
  ADT_029                  Int // Average daily traffic
  YEAR_ADT_030             Int // Year ADT
  DESIGN_LOAD_031          Int // Design load
  APPR_WIDTH_MT_032        Float // Approach width (meters)
  MEDIAN_CODE_033          Int // Median code
  DEGREES_SKEW_034         Int // Degrees of skew
  STRUCTURE_FLARED_035     Int // Structure flared
  RAILINGS_036A            Int // Railings
  TRANSITIONS_036B         Int // Transitions
  APPR_RAIL_036C           Int // Approach rail
  APPR_RAIL_END_036D       Int // Approach rail end
  HISTORY_037              Int // History
  NAVIGATION_038           Int // Navigation
  NAV_VERT_CLR_MT_039      Float // Navigation vertical clearance (meters)
  NAV_HORR_CLR_MT_040      Float // Navigation horizontal clearance (meters)
  OPEN_CLOSED_POSTED_041   String // Open/closed/posted status
  SERVICE_ON_042A          Int // Service on
  SERVICE_UND_042B         Int // Service under
  STRUCTURE_KIND_043A      Int // Structure kind
  STRUCTURE_TYPE_043B      Int // Structure type
  APPR_KIND_044A           Int // Approach kind
  APPR_TYPE_044B           Int // Approach type
  MAIN_UNIT_SPANS_045      Int // Main unit spans
  APPR_SPANS_046           Int // Approach spans
  HORR_CLR_MT_047          Float // Horizontal clearance (meters)
  MAX_SPAN_LEN_MT_048      Float // Maximum span length (meters)
  STRUCTURE_LEN_MT_049     Float // Structure length (meters)
  LEFT_CURB_MT_050A        Float // Left curb width (meters)
  RIGHT_CURB_MT_050B       Float // Right curb width (meters)
  ROADWAY_WIDTH_MT_051     Float // Roadway width (meters)
  DECK_WIDTH_MT_052        Float // Deck width (meters)
  VERT_CLR_OVER_MT_053     Float // Vertical clearance over (meters)
  VERT_CLR_UND_REF_054A    String? // Vertical clearance under reference (nullable)
  VERT_CLR_UND_054B        Float? // Vertical clearance under (nullable)
  LAT_UND_REF_055A         String? // Latitude under reference (nullable)
  LAT_UND_MT_055B          Float? // Latitude under (nullable)
  LEFT_LAT_UND_MT_056      Float? // Left latitude under (nullable)
  DECK_COND_058            Int // Deck condition
  SUPERSTRUCTURE_COND_059  Int // Superstructure condition
  SUBSTRUCTURE_COND_060    Int // Substructure condition
  CHANNEL_COND_061         Int // Channel condition
  CULVERT_COND_062         Int // Culvert condition
  OPR_RATING_METH_063      Int // Operating rating method
  OPERATING_RATING_064     Float // Operating rating
  INV_RATING_METH_065      Int // Inventory rating method
  INVENTORY_RATING_066     Float // Inventory rating
  STRUCTURAL_EVAL_067      Int // Structural evaluation
  DECK_GEOMETRY_EVAL_068   Int // Deck geometry evaluation
  UNDCLRENCE_EVAL_069      Int // Underclearance evaluation
  POSTING_EVAL_070         Int // Posting evaluation
  WATERWAY_EVAL_071        Int // Waterway evaluation
  APPR_ROAD_EVAL_072       Int // Approach road evaluation
  WORK_PROPOSED_075A       Int? // Work proposed (nullable)
  WORK_DONE_BY_075B        Int? // Work done by (nullable)
  IMP_LEN_MT_076           Float? // Improvement length (nullable)
  DATE_OF_INSPECT_090      DateTime // Date of inspection
  INSPECT_FREQ_MONTHS_091  Int? // Inspection frequency (months, nullable)
  FRACTURE_092A            String? // Fracture (nullable)
  UNDERWATER_LOOK_SEE_092B String? // Underwater look/see (nullable)
  SPEC_INSPECT_092C        String? // Special inspection (nullable)
  BRIDGE_IMP_COST_094      Float? // Bridge improvement cost (nullable)
  ROADWAY_IMP_COST_095     Float? // Roadway improvement cost (nullable)
  TOTAL_IMP_COST_096       Float? // Total improvement cost (nullable)
  YEAR_OF_IMP_097          Int? // Year of improvement (nullable)
  OTHER_STATE_CODE_098A    String? // Other state code (nullable)
  OTHER_STATE_PCNT_098B    Float? // Other state percent (nullable)
  OTHR_STATE_STRUC_NO_099  String? // Other state structure number (nullable)
  STRAHNET_HIGHWAY_100     String // STRAHNET highway
  PARALLEL_STRUCTURE_101   Int // Parallel structure
  TRAFFIC_DIRECTION_102    Int // Traffic direction
  TEMP_STRUCTURE_103       Int // Temporary structure
  HIGHWAY_SYSTEM_104       Int // Highway system
  FEDERAL_LANDS_105        Int // Federal lands
  YEAR_RECONSTRUCTED_106   Int? // Year reconstructed (nullable)
  DECK_STRUCTURE_TYPE_107  Int? // Deck structure type (nullable)
  SURFACE_TYPE_108A        Int? // Surface type (nullable)
  MEMBRANE_TYPE_108B       Int? // Membrane type (nullable)
  DECK_PROTECTION_108C     Int? // Deck protection (nullable)
  PERCENT_ADT_TRUCK_109    Float? // Percent ADT trucks (nullable)
  NATIONAL_NETWORK_110     String // National network
  PIER_PROTECTION_111      Int // Pier protection
  BRIDGE_LEN_IND_112       Int // Bridge length indicator
  SCOUR_CRITICAL_113       Int // Scour critical
  FUTURE_ADT_114           Int? // Future ADT (nullable)
  YEAR_OF_FUTURE_ADT_115   Int? // Year of future ADT (nullable)
  MIN_NAV_CLR_MT_116       Float? // Minimum navigation clearance (nullable)
  FED_AGENCY               String? // Federal agency (nullable)
  SUBMITTED_BY             String // Submitted by
  BRIDGE_CONDITION         String // Bridge condition
  LOWEST_RATING            Int // Lowest rating
  DECK_AREA                Float // Deck area
}
